IMAPAccount       austin@austinray.io-account
Host              imap.gmail.com
User              austin@austinray.io
PassCmd           "pass show Mail/austin"
SSLType           IMAPS
SSLVersions       TLSv1.1
CertificateFile   /etc/ssl/certs/ca-certificates.crt

IMAPStore         austin@austinray.io-remote
Account           austin@austinray.io-account

MaildirStore      austin@austinray.io-local
Path              ~/mail/austin@austinray.io/
Inbox             ~/mail/austin@austinray.io/INBOX

Channel           austin@austinray.io-inbox
Master            :austin@austinray.io-remote:
Slave             :austin@austinray.io-local:
Patterns          "INBOX"
Create            Both
Expunge           Both
SyncState         *

Channel           austin@austinray.io-sent
Master            :austin@austinray.io-remote:"[Gmail]/Sent Mail"
Slave             :austin@austinray.io-local:sent
Create            Both
Expunge           Both
SyncState         *

Channel           austin@austinray.io-archive
Master            :austin@austinray.io-remote:"[Gmail]/All Mail"
Slave             :austin@austinray.io-local:archive
Create            Both
Expunge           Both
SyncState         *

Channel           austin@austinray.io-spam
Master            :austin@austinray.io-remote:"[Gmail]/Spam"
Slave             :austin@austinray.io-local:spam
Create            Both
Expunge           Both
SyncState         *

Channel           austin@austinray.io-drafts
Master            :austin@austinray.io-remote:"[Gmail]/Drafts"
Slave             :austin@austinray.io-local:drafts
Create            Both
Expunge           Both
SyncState         *

Group             austin@austinray.io
Channel           austin@austinray.io-inbox
Channel           austin@austinray.io-sent
Channel           austin@austinray.io-archive
Channel           austin@austinray.io-spam
Channel           austin@austinray.io-drafts


